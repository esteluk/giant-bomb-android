<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="harris.GiantBomb" android:versionName="1.1" android:versionCode="110">
    <application android:icon="@drawable/icon" android:label="@string/app_name" android:enabled="true">
        <activity android:name=".VideoList"
                  android:label="@string/app_name" android:icon="@drawable/icon">
        	<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
</activity>
        
    
<activity android:excludeFromRecents="true" android:name=".VidPlayer">
        	<intent-filter>
				<action android:name="android.intent.action.VIEW" />
				<category android:name="android.intent.category.ALTERNATIVE" />
			</intent-filter>
</activity>
</application>
    <uses-sdk android:minSdkVersion="3" />
    <uses-permission android:name="android.permission.INTERNET" /> 
</manifest> 